
CPP= g++
CFLAGS= -O3 -fopenmp -std=c++11
OBJECTS= SampleDecoder.o

# Targets:
all: samplecode

samplecode: $(OBJECTS)
	$(CPP) $(CFLAGS) $(OBJECTS) samplecode.cpp -o main
        
SampleDecoder.o: SampleDecoder.cpp
	$(CPP) $(CFLAGS) -c SampleDecoder.cpp

# Remove:
clean:
	rm -f sampledecode $(OBJECTS)
